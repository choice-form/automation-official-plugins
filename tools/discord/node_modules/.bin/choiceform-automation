#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wester/GitHub/plugins/communication/discord/node_modules/.pnpm/@choiceform+automation-sdk@1.2.1/node_modules/@choiceform/automation-sdk/dist/cli/node_modules:/Users/wester/GitHub/plugins/communication/discord/node_modules/.pnpm/@choiceform+automation-sdk@1.2.1/node_modules/@choiceform/automation-sdk/dist/node_modules:/Users/wester/GitHub/plugins/communication/discord/node_modules/.pnpm/@choiceform+automation-sdk@1.2.1/node_modules/@choiceform/automation-sdk/node_modules:/Users/wester/GitHub/plugins/communication/discord/node_modules/.pnpm/@choiceform+automation-sdk@1.2.1/node_modules/@choiceform/node_modules:/Users/wester/GitHub/plugins/communication/discord/node_modules/.pnpm/@choiceform+automation-sdk@1.2.1/node_modules:/Users/wester/GitHub/plugins/communication/discord/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/wester/GitHub/plugins/communication/discord/node_modules/.pnpm/@choiceform+automation-sdk@1.2.1/node_modules/@choiceform/automation-sdk/dist/cli/node_modules:/Users/wester/GitHub/plugins/communication/discord/node_modules/.pnpm/@choiceform+automation-sdk@1.2.1/node_modules/@choiceform/automation-sdk/dist/node_modules:/Users/wester/GitHub/plugins/communication/discord/node_modules/.pnpm/@choiceform+automation-sdk@1.2.1/node_modules/@choiceform/automation-sdk/node_modules:/Users/wester/GitHub/plugins/communication/discord/node_modules/.pnpm/@choiceform+automation-sdk@1.2.1/node_modules/@choiceform/node_modules:/Users/wester/GitHub/plugins/communication/discord/node_modules/.pnpm/@choiceform+automation-sdk@1.2.1/node_modules:/Users/wester/GitHub/plugins/communication/discord/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@choiceform/automation-sdk/dist/cli/index.js" "$@"
else
  exec node  "$basedir/../@choiceform/automation-sdk/dist/cli/index.js" "$@"
fi
